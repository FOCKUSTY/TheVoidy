    "typescript-eslint": "^8.19.1"
  },
  "workspaces": [
    "bot",
    "bots/*",
    "utility/*",
    "packages/*"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "node"
    ]
  }
