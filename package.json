{
  "name": "voidbotts",
  "version": "1.3.1-alpha.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:slash": "nodemon src/discord/slash.commands.ts",
    "dev": "set NODE_ENV=dev&& set BOT=all&& nodemon src/start.bot.ts",
    "dev:telegram": "set NODE_ENV=dev&& set BOT=telegram&& nodemon src/start.bot.ts",
    "dev:discord": "set NODE_ENV=dev&& set BOT=discord&& nodemon src/start.bot.ts",
    "start:slash": "node -r tsconfig-paths/register dist/src/discord/slash.commands.js",
    "start:builded": "set NODE_ENV=prod&& set BOT=all&& node -r tsconfig-paths/register dist/src/start.bot.js",
    "start": "set NODE_ENV=prod&& yarn run update && yarn run build && yarn run start:slash && yarn run start:builded",
    "update": "fockupdater update",
    "lint": "eslint eslint.config.mjs --no-warn-ignored src/**/*.{js,mjs,cjs,ts} --fix",
    "build": "tsc"
  },
  "author": "FOCKUSTY",
  "license": "MIT",
  "dependencies": {
    "@thevoidcommunity/the-void-database": "1.0.0-alpha.1",
    "date-fns": "^2.30.0",
    "discord.js": "^14.15.3",
    "f-formatter": "^0.3.1",
    "fock-logger": "2.0.5",
    "fock-updater": "^1.4.1-beta",
    "ollama": "0.5.11",
    "random-js": "^2.1.0",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^20.16.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.19.1"
  }
}
