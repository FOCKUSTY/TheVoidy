  "license": "MIT",
  "scripts": {
    "build": "pnpm run build:packages && pnpm run build:utility && pnpm run build:bots && pnpm run build:bot",
    "build:bots": "pnpm --filter {./bots/**} build",
    "build:packages": "pnpm --filter {./packages/**} build",
    "build:utility": "pnpm --filter {./utility/**} build",
    "build:bot": "tsc",
    "set:prod": "set NODE_ENV=prod&& set BOT=all",
    "start:bot": "set NODE_ENV=prod&& set BOT=all&& node dist/bot/start.bot",
    "start": "set NODE_ENV=prod&& set BOT=all&& pnpm run build && pnpm run linter && pnpm run start:bot",
    "linter": "pnpm run lint && pnpm run lint:fix",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . -c -w"
  },
  "dependencies": {
    "@thevoidcommunity/the-void-database": "catalog:",
    "@voidy/discord": "workspace:^",
    "@voidy/services": "workspace:^",
    "@voidy/telegram": "workspace:^",
    "@voidy/develop": "workspace:^",
    "date-fns": "catalog:",
    "discord.js": "catalog:",
    "openai": "catalog:",
    "f-formatter": "catalog:",
    "random-js": "catalog:",
    "telegraf": "catalog:"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "cross-env": "^7.0.3",
    "@voidy/types": "workspace:^",
    "dotenv": "catalog:",
    "@eslint/js": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "globals": "catalog:",
    "nodemon": "catalog:",
    "ts-node": "catalog:",
    "tsc-alias": "catalog:",
    "tsconfig-paths": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "workspaces": [
    "bot",
    "bots/**",
    "utility/**",
    "packages/**"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "node"
    ]
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39"
