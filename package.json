    "f-formatter": "0.3.1",
    "fock-logger": "6.3.1",
    "openai": "4.83.0",
    "random-js": "^2.1.0",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
